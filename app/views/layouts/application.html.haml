!!!
%html
  %head
    %title Aquapark
    = stylesheet_link_tag    'application', media: 'all'
    = stylesheet_link_tag 'mdb.min.css'
    = favicon_link_tag 'Aquapark', href: '/images/brand.png'
    = javascript_include_tag "application", "data-turbolinks-track" => true
    = csrf_meta_tags
    %link{ href: 'http://fonts.googleapis.com/css?family=Archivo+Black&subset=latin,latin-ext', rel: 'stylesheet', type: 'text/css' }
    %link{ href: 'https://fonts.googleapis.com/icon?family=Material+Icons', rel: 'stylesheet' }

  %body#frontend{"data-spy" => "scroll", "data-target" => ".navbar", "data-offset" =>"50"}
    = render '/layouts/navbar'
    = yield
